<h3 class = 'links_color'>Reviews</h3>
<% @post.comments.each do |comment| %>
    <div class="well">
        <p><strong><%= comment.username %> </strong>: <%= comment.body %>
        <%= link_to 'Delete',[comment.post, comment], 
        method: :delete, data: { confirm: "Are you sure?" } %></p>
    </div>
<% end %>